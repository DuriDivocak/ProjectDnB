PSVERSION=2
PSVERSION_WARP=2
[preset00]

fDecay=0.98
zoom=1.0
rot=0.0
warp=0.0



// Custom Shape Parameters
per_frame_1= q1 = 0.5 + 0.2 * sin(time * 3); // Pulsating radius
per_frame_2= q2 = 0.5; // Center X position
per_frame_3= q3 = 0.5; // Center Y position
per_frame_4= zoom = 1.0 * 0.2;

warp_1=`shader_body
warp_2=`{
warp_3=`    // sample a pixel from the previous frame.  
warp_4=`    // uv coord is slightly warped (driven by the per-vertex equations),
warp_5=`    //   and is what creates the main "movement" in our preset.
warp_6=`    ret = tex2D( sampler_main, uv ).xyz;
warp_7=`
warp_8=`    // darken over time
warp_9=`    ret *= 0.97;
warp_10=`}

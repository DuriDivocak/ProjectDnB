MILKDROP_PRESET_VERSION=201
PSVERSION=4
PSVERSION_WARP=2
PSVERSION_COMP=4
[preset00]

// Global Settings
fDecay=0.98
zoom=1.0
rot=0.0
warp=0.0

wave_a=0
wave_r=0
wave_g=0
wave_b=0
wave_y=100
nWaveMode=5
bAdditiveWaves=0
bWaveDots=0
bWaveThick=1
bModWaveAlphaByVolume=1
bMaximizeWaveColor=1

// Custom Wave 0 (Smooth & Controlled Waveform)
wavecode_0_enabled=1
wavecode_0_bSpectrum=1
wavecode_0_samples=512
wavecode_0_scaling=1.0
wavecode_0_r=1.0
wavecode_0_g=0.0
wavecode_0_b=1.0
wavecode_0_a=1.0
wavecode_0_x=2.0
wavecode_0_y=1.0

wave_0_per_point1=y = (sample - 0.4) * 1.5;
wave_0_per_point2=x = 0.1 + 0.2 * sin(sample * 3.14159) + value1 * 0.5;

wavecode_1_enabled=1
wavecode_1_bSpectrum=1
wavecode_1_samples=512
wavecode_1_scaling=1.0
wavecode_1_r=0.0
wavecode_1_g=1.0
wavecode_1_b=1.0
wavecode_1_a=1.0
wavecode_1_x=2.0
wavecode_1_y=1.0

wave_1_per_point1=y = (sample - 0.4) * 1.5;
wave_1_per_point2=x = 0.9 + 0.2 * sin(-sample * 3.14159) - value1 * 0.5;

shapecode_0_enabled=1
shapecode_0_sides=6
shapecode_0_x=0.5
shapecode_0_y=0.1
shapecode_0_r=1.00000
shapecode_0_g=1
shapecode_0_b=1.00000
shapecode_0_a=0
shapecode_0_rad=0.01
shapecode_0_border_r=1.000
shapecode_0_border_g=1.000
shapecode_0_border_b=1.000
shapecode_0_border_a=1.000

shape_0_per_frame1=rad = 0.01 + bass_att * 0.15;
shape_0_per_frame2=border_r = 0.5 + 0.5 * sin(time * 2 + bass * 5);
shape_0_per_frame3=border_g = 0.2 + 0.8 * cos(time * 3 + bass * 5);
shape_0_per_frame4=x = 0.5 + 0.1 * sin(time * 3 + bass_att);
shape_0_per_frame5=y = 0.3 + 0.1 * cos(time * 3 + bass_att);

shapecode_1_enabled=1
shapecode_1_sides=6
shapecode_1_x=0.5
shapecode_1_y=0.1
shapecode_1_r=0.00000
shapecode_1_g=0
shapecode_1_b=1.00000
shapecode_1_a=0
shapecode_1_rad=0.01
shapecode_1_border_r=1.000
shapecode_1_border_g=1.000
shapecode_1_border_b=1.000
shapecode_1_border_a=1.000

shape_1_per_frame1=rad = 0.01 + bass_att * 0.15;
shape_1_per_frame2=border_r = 0.5 + 0.5 * sin(time * 2 + bass * 5);
shape_1_per_frame3=border_g = 0.2 + 0.8 * cos(time * 3 + bass * 5);
shape_1_per_frame4=x = 0.5 - 0.1 * sin(time * 3 + bass_att);
shape_1_per_frame5=y = 0.3 - 0.1 * cos(time * 3 + bass_att);


// Per Frame Code
per_frame_1=zoom = 1.0 + bass_att * 0.05;
per_frame_2=rot = 0.01 * sin(treb * 4);

